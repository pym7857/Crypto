import java.io.File;
import java.io.FileReader;

public class Main {

	public static void main(String[] args) {
		
		try {
			File file = new File("C:\\excelExample\\ciphertext.txt");
			FileReader fr = new FileReader(file);
			
			int cur = 0;
			
			while( (cur = fr.read()) != -1) { // -1Àº EOF ÀÇ¹Ì
				System.out.print( (char)cur );
			}
		} catch(Exception e) {
			e.printStackTrace();
		}
		
	}

}
